    "esbuild-sass-plugin": "^2.12.0",
    "rollup": "^4.1.4",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typora": "npm:@typora-community-plugin/rollup-plugin-typora@^1.0.3",
    "sass": "^1.69.5",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "katex": "^0.16.19",
    "markmap-common": "^0.17.0",
    "markmap-lib": "^0.17.0",
    "markmap-render": "^0.17.0",
    "markmap-view": "^0.17.0"
